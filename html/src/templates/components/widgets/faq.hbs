{{#compare show '==' 'true' }}
<div class="accordion-item custom-accordion border-0 ">
   <h4 class="accordion-header rounded-0 p-0" id="heading{{uniqid}}">
      <button class="accordion-button faq-btn p-0 text-capitalize gap-3" type="button"
         data-bs-toggle="collapse" data-bs-target="#collapse{{inid}}" aria-expanded="true"
         aria-controls="collapse{{inid}}">
         {{faq-title}}
      </button>
   </h4>
   <div id="collapse{{inid}}" class="accordion-collapse collapse show" aria-labelledby="heading{{uniqid}}"
      data-bs-parent="#main-faq">
      <div class="accordion-body p-0">
         <p class="mb-0 mt-3 font-size-14">{{faq-desc}}</p>
      </div>
   </div>
</div>
{{else}}
<div class="accordion-item custom-accordion border-0">
   <h4 class="accordion-header rounded-0 p-0" id="heading{{uniqid}}">
      <button class="accordion-button p-0 faq-btn text-capitalize collapsed gap-3" type="button"
         data-bs-toggle="collapse" data-bs-target="#collapse{{inid}}" aria-expanded="true"
         aria-controls="collapse{{inid}}">
         {{faq-title}}
      </button>
   </h4>
   <div id="collapse{{inid}}" class="accordion-collapse collapse" aria-labelledby="heading{{uniqid}}"
      data-bs-parent="#main-faq">
      <div class="accordion-body p-0">
         <p class="mb-0 mt-3 font-size-14">{{faq-desc}}</p>
      </div>
   </div>
</div>
{{/compare}}